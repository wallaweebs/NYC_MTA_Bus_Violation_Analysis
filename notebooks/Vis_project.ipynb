    {
      "cell_type": "code",
      "source": [
        "violations_gdf = gpd.GeoDataFrame(\n",
        "  violations_geo,\n",
        "  geometry=gpd.points_from_xy(\n",
        "      violations_geo['Violation Longitude'],\n",
        "      violations_geo['Violation Latitude']\n",
        "  ),\n",
        "  crs='EPSG:4326'\n",
        ")\n",
        "\n",
        "viol_join = gpd.sjoin(violations_gdf, areas_geo.set_geometry('buffer'), how='left', predicate='within')"
      ],
      "metadata": {
        "id": "Kp-H17Llmo_p"
      },
      "execution_count": 136,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "viol_join['Quarter'] = viol_join['Month'].dt.to_period('Q').astype(str)\n",
        "\n",
        "quarterly = viol_join.groupby(['area', 'Quarter']).size().reset_index(name='violations')"
      ],
      "metadata": {
        "id": "UmUrj3zlmxdA"
